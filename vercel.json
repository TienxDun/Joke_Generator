{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "headers": {
        "x-vercel-protection-bypass": "true"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}